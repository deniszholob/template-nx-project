{
  "name": "template-nx-project",
  "version": "0.0.0",
  "license": "GPL-3",
  "author": "Denis Zholob",
  "description": "template_nx_project_description",
  "repository": "github:deniszholob/template-nx-project",
  "private": true,
  "engines": {
    "node": ">= 18.19.1"
  },
  "scripts": {
    "i": "pnpm install",
    "nx:clear-cache": "nx reset",
    "start": "nx run template-nx-project:serve --port=4200",
    "storybook": "nx run template-nx-project:storybook",
    "storybook:test": "nx run template-nx-project:test-storybook",
    "storybook:docs": "nx run template-nx-project:serve-storybook --docs",
    "storybook:doctor": "npx storybook doctor --config-dir .storybook",
    "start:prod": "pnpm run build && http-server -g -p 5555 --cors -c-1 dist/browser -o",
    "check-circ-deps": "madge ./src -c --extensions ts",
    "dep-graph": "nx dep-graph",
    "inferred-tasks": "nx show project template-nx-project --web ",
    "lint:check": "nx lint",
    "lint:fix": "nx lint --fix",
    "lint:err": "nx lint --quiet",
    "format:check": "nx format:check",
    "format:fix": "nx format:write",
    "affected:lint": "nx affected:lint",
    "affected:lint:fix": "nx affected:lint --fix",
    "affected:test": "nx affected:test",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "test": "nx test --codeCoverage",
    "test:view-coverage": "node -e \"require('child_process').exec(process.platform === 'win32' ? 'start' : process.platform === 'darwin' ? 'open' : 'xdg-open' + ' coverage/index.html')\"",
    "e2e:open": "nx e2e template-nx-project-e2e --port 8080 --watch=true",
    "e2e:ci": "nx e2e template-nx-project-e2e --env.grepTags='' --browser chrome",
    "e2e:integration": "nx e2e --env.grepTags='@integration' --browser chrome",
    "patchModifyDate": "node .github/ci/date-patch.js",
    "build": "nx build",
    "build:gh-prod": "pnpm run patchModifyDate && nx build template-nx-project --base-href='/template-nx-project/'",
    "pnpmVersionPatch": "pnpm version patch",
    "upgrade:nx:create": "nx migrate latest",
    "upgrade:npm:i": "pnpm install --no-frozen-lockfile",
    "upgrade:nx:apply": "nx migrate --run-migrations",
    "upgrade:storybook": "npx storybook@latest upgrade --config-dir ./.storybook",
    "upgrade:storybook-auto": "npx storybook@latest automigrate --config-dir ./.storybook",
    "storybook:build": "nx run template-nx-project:build-storybook",
    "storybook:playwright:ci": "pnpx playwright install --with-deps chromium",
    "storybook:test:ci": "pnpx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpx http-server dist-storybook --port 6006\" \"pnpx wait-on tcp:127.0.0.1:6006 && test-storybook\"",
    "pre-commit": "npx lint-staged",
    "prepare": "husky install",
    "nx": "nx",
    "set-permissions": "chmod +x ./node_modules/husky/lib/bin.js && chmod +x ./node_modules/.bin/prettier"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@angular/animations": "19.1.1",
    "@angular/common": "19.1.1",
    "@angular/compiler": "19.1.1",
    "@angular/core": "19.1.1",
    "@angular/forms": "19.1.1",
    "@angular/platform-browser": "19.1.1",
    "@angular/platform-browser-dynamic": "19.1.1",
    "@angular/router": "19.1.1",
    "@babel/runtime": "^7.23.9",
    "@nx/angular": "20.3.2",
    "rxjs": "~7.8.1",
    "tslib": "^2.3.0",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.0.5",
    "@angular-devkit/core": "19.0.5",
    "@angular-devkit/schematics": "19.0.5",
    "@angular-eslint/eslint-plugin": "19.0.2",
    "@angular-eslint/eslint-plugin-template": "19.0.2",
    "@angular-eslint/template-parser": "19.0.2",
    "@angular/cli": "~19.0.0",
    "@angular/compiler-cli": "19.1.1",
    "@angular/language-service": "19.1.1",
    "@nx/cypress": "20.3.2",
    "@nx/eslint": "20.3.2",
    "@nx/eslint-plugin": "20.3.2",
    "@nx/jest": "20.3.2",
    "@nx/js": "20.3.2",
    "@nx/storybook": "20.3.2",
    "@nx/web": "20.3.2",
    "@nx/workspace": "20.3.2",
    "@schematics/angular": "19.1.1",
    "@storybook/addon-a11y": "8.5.0",
    "@storybook/addon-essentials": "8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/angular": "8.5.0",
    "@storybook/core-common": "8.5.0",
    "@storybook/core-server": "8.5.0",
    "@storybook/manager-api": "8.5.0",
    "@storybook/test": "^8.5.0",
    "@storybook/test-runner": "^0.20.1",
    "@storybook/theming": "8.5.0",
    "@storybook/types": "8.5.0",
    "@tailwindcss/forms": "^0.5.7",
    "@types/jest": "29.5.12",
    "@types/node": "^18.16.9",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cypress": "13.13.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-cypress": "2.13.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "0.11.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-preset-angular": "14.4.2",
    "lint-staged": "^15.2.0",
    "madge": "^8.0.0",
    "ng-mocks": "14.13.1",
    "nx": "20.3.2",
    "playwright": "^1.49.1",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-attributes": "^1.0.0",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "storybook": "8.5.0",
    "storybook-addon-angular-router": "^1.10.1",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.6.3"
  }
}
