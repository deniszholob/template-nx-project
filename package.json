{
  "name": "template-nx-project",
  "version": "0.0.0",
  "license": "GPL-3",
  "author": "Denis Zholob",
  "description": "template_nx_project_description",
  "repository": "github:deniszholob/template-nx-project",
  "private": true,
  "engines": {
    "node": ">= 18.12.1",
    "npm": ">= 8.19.2"
  },
  "scripts": {
    "ci": "npm ci",
    "i": "npm i",
    "dep-graph": "nx dep-graph",
    "storybook": "nx run template-nx-project:storybook",
    "start": "nx run template-nx-project:serve --port=4200",
    "start:prod": "npm run build && http-server -g -p 5555 --cors -c-1 public -o",
    "nx:clear-cache": "nx reset",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "lint:err": "nx lint --quiet",
    "format:fix": "nx format:write",
    "format:check": "nx format:check",
    "test": "nx test --codeCoverage",
    "test:view-coverage": "cd coverage && index.html",
    "patchModifyDate": "node .github/ci/date-patch.js",
    "build": "nx build",
    "build:gh-prod": "npm run patchModifyDate && nx build template-nx-project --base-href='/template-nx-project/'",
    "npmVersionPatch": "npm version patch",
    "nx:update:create": "nx migrate latest",
    "nx:update:apply": "nx migrate --run-migrations",
    "pre-commit": "npx lint-staged",
    "prepare": "husky install",
    "nx": "nx"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@angular/animations": "16.2.10",
    "@angular/common": "16.2.10",
    "@angular/compiler": "16.2.10",
    "@angular/core": "16.2.10",
    "@angular/forms": "16.2.10",
    "@angular/platform-browser": "16.2.10",
    "@angular/platform-browser-dynamic": "16.2.10",
    "@angular/router": "16.2.10",
    "@nx/angular": "16.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "16.2.7",
    "@angular-devkit/core": "16.2.7",
    "@angular-devkit/schematics": "16.2.7",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.2.0",
    "@angular/compiler-cli": "16.2.10",
    "@angular/language-service": "16.2.10",
    "@nx/cypress": "16.10.0",
    "@nx/eslint-plugin": "16.10.0",
    "@nx/jest": "16.10.0",
    "@nx/js": "16.10.0",
    "@nx/linter": "16.10.0",
    "@nx/storybook": "16.10.0",
    "@nx/web": "16.10.0",
    "@nx/workspace": "16.10.0",
    "@schematics/angular": "16.2.7",
    "@storybook/addon-essentials": "7.1.0",
    "@storybook/angular": "7.1.0",
    "@storybook/core-common": "^7.0.24",
    "@storybook/core-server": "7.1.0",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.60.1",
    "@typescript-eslint/parser": "5.60.1",
    "cypress": "^13.0.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "2.13.4",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "lint-staged": "^15.2.0",
    "ng-mocks": "^14.11.0",
    "nx": "16.10.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  }
}
