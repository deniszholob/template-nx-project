{
  "name": "template-nx-project",
  "version": "0.0.0",
  "license": "GPL-3",
  "author": "Denis Zholob",
  "description": "template_nx_project_description",
  "repository": "github:deniszholob/template-nx-project",
  "private": true,
  "engines": {
    "node": ">= 18.12.1",
    "npm": ">= 8.19.2"
  },
  "scripts": {
    "i": "npm i",
    "dep-graph": "nx dep-graph",
    "storybook": "nx run template-nx-project:storybook",
    "start": "nx run template-nx-project:serve --port=4200",
    "start:prod": "npm run build && http-server -g -p 5555 --cors -c-1 public -o",
    "nx:clear-cache": "nx reset",
    "lint": "nx lint",
    "lint:fix": "nx lint --fix",
    "lint:err": "nx lint --quiet",
    "format:fix": "nx format:write",
    "format:check": "nx format:check",
    "test": "nx test --codeCoverage",
    "test:view-coverage": "cd coverage && index.html",
    "patchModifyDate": "node .github/ci/date-patch.js",
    "build": "nx build",
    "build:gh-prod": "npm run patchModifyDate && nx build template-nx-project --base-href='/template-nx-project/'",
    "npmVersionPatch": "npm version patch",
    "nx:update:create": "nx migrate latest",
    "nx:update:apply": "nx migrate --run-migrations",
    "nx": "nx"
  },
  "dependencies": {
    "@angular/animations": "~16.1.0",
    "@angular/common": "~16.1.0",
    "@angular/compiler": "~16.1.0",
    "@angular/core": "~16.1.0",
    "@angular/forms": "~16.1.0",
    "@angular/platform-browser": "~16.1.0",
    "@angular/platform-browser-dynamic": "~16.1.0",
    "@angular/router": "~16.1.0",
    "@nx/angular": "16.5.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~16.1.0",
    "@angular-devkit/core": "~16.1.0",
    "@angular-devkit/schematics": "~16.1.0",
    "@angular-eslint/eslint-plugin": "~16.0.0",
    "@angular-eslint/eslint-plugin-template": "~16.0.0",
    "@angular-eslint/template-parser": "~16.0.0",
    "@angular/cli": "~16.1.0",
    "@angular/compiler-cli": "~16.1.0",
    "@angular/language-service": "~16.1.0",
    "@nx/cypress": "16.5.5",
    "@nx/eslint-plugin": "16.5.5",
    "@nx/jest": "16.5.5",
    "@nx/js": "16.5.5",
    "@nx/linter": "16.5.5",
    "@nx/storybook": "^16.5.5",
    "@nx/web": "16.5.5",
    "@nx/workspace": "16.5.5",
    "@schematics/angular": "~16.1.0",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/angular": "^7.0.24",
    "@storybook/core-common": "^7.0.24",
    "@storybook/core-server": "^7.0.24",
    "@types/jest": "^29.4.0",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cypress": "^12.16.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "http-server": "^14.1.1",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-preset-angular": "~13.1.0",
    "ng-mocks": "^14.11.0",
    "nx": "16.5.5",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-attributes": "^0.0.5",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.1.3"
  }
